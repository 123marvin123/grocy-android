<androidx.cardview.widget.CardView
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  android:layout_width="match_parent"
  android:layout_height="wrap_content"
  style="@style/Widget.Material3.CardView.Filled"
  app:cardBackgroundColor="@android:color/transparent"
  app:cardCornerRadius="12dp"
  app:cardElevation="0dp"
  android:layout_marginHorizontal="16dp">

  <xyz.zedler.patrick.grocy.view.MaterialTimelineView
    android:id="@+id/timeline_view"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@android:color/transparent"
    android:padding="16dp"
    app:timeline_position="middle"
    app:timeline_type="line"
    app:timeline_bottom_radio_color="?attr/colorOutline"
    app:timeline_top_radio_color="?attr/colorOutline"
    app:timeline_line_color="?attr/colorOutline">


  </xyz.zedler.patrick.grocy.view.MaterialTimelineView>

</androidx.cardview.widget.CardView>